<div class="pull-right alert">
	<div class ="jumbotron box">
	  <%= form_for :doctor,url: doctors_path do |f| %>
	<p>
	   <%= f.label :Name %>
	   <%= f.text_field :name %> 	
	</p>
	 
	<p>
	   <%= f.label :Email%>
	   <%= f.text_field :email%>
	</p>

	<p>
		<%= f.label :Password%>
		<%=f.text_field :passwd%>
	</p>

	<p>
	    <%= f.label :Years_Of_Experience%>
	    <%= f.text_field :years_of_exp%>	
	</p>

	<p>
		<%= f.submit :SignUp%>
	</p>
	  
	   <% end %>
	 </div>
</div>